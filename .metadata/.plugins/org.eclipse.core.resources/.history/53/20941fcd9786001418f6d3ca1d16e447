
public class Character {

	private String name;
	private double HP;
	private double dexterity;
	private Weapons weapon;
	private double strenght;
	private double speed;

	public String getName() {
		return name;
	}
	
	public double getHP() {
		return HP;
	}
	
	public double getDexterity() {
		return dexterity;
	}
	
}

